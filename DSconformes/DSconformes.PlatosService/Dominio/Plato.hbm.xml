<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns ="urn:nhibernate-mapping 2.2"
                   assembly ="DSconformes"
                   namespace ="DSconformes.Dominio"
                   default-lazy="false">

  <class name ="Plato" table="t_plato">
    <id name="id_plato" column="id_plato">
      <generator class="increment"  />
    </id>
    <many-to-one name ="id_categoria" colum="id_categoria" />
    <property name="nombre" column="dni" />
    <property name="costo" column="costo" />
    <property name="descripcion" column="descripcion" />


  </class>
</hibernate-mapping>

